<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<html>
<body>
	<h2>${headerValue}</h2>
	
		<table>
			<c:forEach var="student" items="${students}" varStatus="status">
				<tr>	
					<td> ${student.id}</td>
					<td> ${student.name}</td>	
				</tr>
			</c:forEach>
		</table>
		
</body>

</html>